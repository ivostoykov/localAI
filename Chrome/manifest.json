{
    "manifest_version": 3,
    "name": "Local AI helper",
    "version": "1.27.22",
    "options_page": "options.html",
    "permissions": [
        "activeTab",
        "storage",
        "contextMenus"
    ],
    "host_permissions": [
        "*://*/*"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "jslib/sessions.js",
                "jslib/solomd2html.js",
                "jslib/utils.js",
                "jslib/stt.js",
                "lai-main.js",
                "content.js"
            ],
            "run_at": "document_start"
        }
    ],
    "action": {},
    "icons": {
        "16": "img/icon16.png",
        "48": "img/icon48.png",
        "128": "img/icon128.png"
    },
    "web_accessible_resources": [
        {
            "resources": [
                "sidebar.html",
                "css/button.css",
                "css/sidebar.css",
                "css/aioutput.css",
                "lai-main.js",
                "img/*.svg"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ]
}